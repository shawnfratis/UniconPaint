######################Main module for Unicon Draw

procedure fstarin(at)

a := at[1]
b := at[2]
c := at[3]

d := at[4]
e := at[5]
f := at[6]

g := at[7]
h := at[8]
i := at[9]

j := at[10]
k := at[11]
l := at[12]
m := at[13]
n := at[14]
o := at[15]

#fstar(20, 245, 330, 4, 8, 0.47, &pi /2)
#fstar(20, 165, 330, 5, 5, 0.35, 4 * &pi / 5)
#fstar(20,165,330,5,5,0.35,4*&pi/5)
#fstar(20, 245, 330, 4, 8, 0.47,2)

fstar(a||b||c, d||e||f, g||h||i ,j ,k ,l||m||n, o)

return()

end



procedure fstar(x, y, extent, vertices, phases, ratio, incr)

theta := 0

every i := 0 to vertices * (vertices-1)  ^ (phases - 1) do {

resid := i

factors := 0

until (resid % (vertices - 1) ~= 0) | (factors >= (phases - 1)) do {

resid /:= (vertices - 1)

factors +:= 1
}

length := extent * ratio ^ (phases - factors - 1)

x +:= length * cos(theta)

y +:= length * sin(theta)

if i > 0 then

DrawLine(xprev, yprev, x, y)

xprev:= x
yprev:= y
#theta +:= incr
theta +:= &pi/incr
}
return

end


